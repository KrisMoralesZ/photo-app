<h1>Users</h1>

<div>
  <table class="table table-striped">
    <thead>
    <tr>
      <th>Name</th>
      <th>Last Name</th>
    </tr>
    </thead>
    <tbody id="usersTable">
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.lastname %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>